# ==================== 数据库认证信息 ====================

# Milvus 认证
MILVUS_USER=root
MILVUS_PASSWORD=your_milvus_password
# Milvus token（如果使用token认证）
MILVUS_TOKEN=

# MongoDB 认证
MONGODB_USER=admin
MONGODB_PASSWORD=your_mongodb_password
# MongoDB 认证数据库
MONGODB_AUTH_SOURCE=admin
# MongoDB 连接字符串（可选，如果提供则优先使用）
MONGODB_URI=

# MySQL 认证
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password

# Neo4j 认证
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password

# Redis 认证
REDIS_PASSWORD=your_redis_password
# Redis 用户名（Redis 6.0+支持）
REDIS_USERNAME=

# MinIO 认证
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=your_minio_secret_key

# ==================== AI 模型 API Keys ====================

# 本地Embedding模型（如果本地服务需要认证，请设置API Key）
EMBEDDING_API_KEY=

# OpenAI API（可选）
OPENAI_API_KEY=sk-xxx
OPENAI_ORG_ID=

# 智谱 AI (Zhipu)（可选）
ZHIPU_API_KEY=your_zhipu_api_key

# 阿里云通义千问 (Qwen)（可选）
QWEN_API_KEY=your_qwen_api_key
QWEN_API_SECRET=

# 谷歌 (Gemini) （可选）
GEMINI_API_KEY=your_gemini_api_key

# 百度千帆 (ERNIE)（可选）
BAIDU_API_KEY=your_baidu_api_key
BAIDU_SECRET_KEY=your_baidu_secret_key

# DeepSeek V3.2（可选）
# 模型: deepseek-chat (非思考模式), deepseek-reasoner (思考模式)
DEEPSEEK_API_KEY=your_deepseek_api_key

# Anthropic Claude（可选）
ANTHROPIC_API_KEY=your_anthropic_api_key

# Cohere (Reranker)（可选）
COHERE_API_KEY=your_cohere_api_key

# Jina AI (Reranker)（可选）
JINA_API_KEY=your_jina_api_key

# ==================== 第三方服务 ====================

# MinerU 服务（PDF解析）
MINERU_API_KEY=

# ==================== 系统配置 ====================

# 应用环境: development, production, testing
APP_ENV=development

# 调试模式
DEBUG=true

# 应用密钥（用于加密等）
APP_SECRET_KEY=your_secret_key_change_in_production

# JWT 密钥
JWT_SECRET_KEY=your_jwt_secret_key

# 加密盐值
ENCRYPTION_SALT=your_encryption_salt
